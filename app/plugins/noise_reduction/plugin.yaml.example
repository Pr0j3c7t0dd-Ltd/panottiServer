name: noise_reduction
version: "1.0.0"
enabled: true
dependencies: []
config:
  output_directory: "data/cleaned_audio"
  noise_reduce_factor: 0.5
  wiener_alpha: 1.2
  highpass_cutoff: 80
  spectral_floor: 0.12
  smoothing_factor: 2
  max_concurrent_tasks: 4
  # If true => time-domain bleed removal; if false => spectral subtraction
  time_domain_subtraction: false
  # For alignment
  use_fft_alignment: true
  alignment_chunk_seconds: 10