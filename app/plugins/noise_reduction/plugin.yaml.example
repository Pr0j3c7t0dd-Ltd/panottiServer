name: "noise_reduction"
version: "1.0.0"
enabled: true
dependencies: []
config:
  output_directory: "data/cleaned_audio"
  noise_reduce_factor: 0.8  # Slightly reduced to preserve more speech
  wiener_alpha: 2.0  # More balanced Wiener filtering
  highpass_cutoff: 85  # Lower cutoff to preserve more voice frequencies
  spectral_floor: 0.06  # Slightly higher floor to reduce musical noise
  smoothing_factor: 3  # Increased smoothing for better noise estimation
  max_concurrent_tasks: 4