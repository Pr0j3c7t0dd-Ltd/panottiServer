name: "noise_reduction"
version: "1.0.0"
enabled: true
dependencies: []
config:
  noise_reduce_factor: 0.2  # Very light noise reduction
  wiener_alpha: 0.8        # Minimal filtering
  spectral_floor: 0.15     # Higher floor preserves more speech
  highpass_cutoff: 50      # Only remove very low rumble
  highpass_order: 2        # Very gentle slope
  stft_window_size: 2048   # Standard for speech
  stft_overlap_percent: 75 # Smooth transitions
  smoothing_factor: 1      # Minimal smoothing