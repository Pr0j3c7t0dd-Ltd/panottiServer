[run]
branch = True
source = app
omit =
    tests/*
    app/**/tests/*
    **/test_*.py
    **/__init__.py

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    if self.debug:
    raise NotImplementedError
    if __name__ == .__main__.:
    pass
    raise ImportError
    except ImportError:
    \.\.\.

ignore_errors = True

[html]
directory = coverage/html 